{
    "name": "solidry/api-generator",
    "description": "PHP-code generator (based on OAS) for Laravel framework, with complete support of JSON-API data format",
    "type": "api-generator",
    "keywords": [
        "json-api",
        "jsonapi",
        "openapi",
        "open-api",
        "generator",
        "laravel",
        "json",
        "api"
    ],
    "homepage": "https://github.com/SoliDry/api-generator",
    "support": {
        "issues": "https://github.com/SoliDry/api-generator/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Kushman",
            "email": "arthurkushman@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "lcobucci/jwt": "^3.2",
        "league/fractal": "~0.14",
        "nwidart/laravel-modules": ">=3.1",
        "php": ">=7.0",
        "predis/predis": "^1.1",
        "raml-org/raml-php-parser": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "SoliDry\\": "src",
            "SoliDryTest\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\":"Modules/"
        }
    },
    "require-dev": {
        "laravel/framework": ">=5.3",
        "phpunit/phpunit": ">=6.5",
        "mockery/mockery": "~1.0",
        "codeception/codeception": "^2.4",
        "fzaninotto/faker": "^1.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
